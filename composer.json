{
	"name": "ozdemirburak/laravel-5-simple-cms",
	"description": "Simple Laravel 5 CMS (Content Management System) for starters.",
	"keywords": ["php", "laravel", "cms"],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=7.1.3",
		"anhskohbo/no-captcha": "~3.0",
		"barryvdh/laravel-elfinder": "~0.3",
		"barryvdh/laravel-snappy": "~0.4",
		"cviebrock/eloquent-sluggable": "~4.3",
		"fideloper/proxy": "~4.0",
		"h4cc/wkhtmltoimage-amd64": "~0.12",
		"h4cc/wkhtmltopdf-amd64": "~0.12",
		"laracasts/flash": "~3.0",
		"laravel/framework": "5.7.*",
		"laravel/tinker": "~1.0",
		"predis/predis": "~1.1",
		"spatie/laravel-analytics": "~3.4",
		"yajra/laravel-datatables-buttons": "~4.0",
		"yajra/laravel-datatables-oracle": "~8.0"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "~3.2",
		"barryvdh/laravel-ide-helper": "~2.5",
		"beyondcode/laravel-dump-server": "^1.2",
		"doctrine/dbal": "~2.8",
		"filp/whoops": "~2.2",
		"fzaninotto/faker": "~1.8",
		"laravel/browser-kit-testing": "~4.0",
		"mockery/mockery": "~1.2",
		"nunomaduro/collision": "~1.1",
		"phpunit/phpunit": "~7.0"
	},
	"autoload": {
		"classmap": [
			"database/seeds",
			"database/factories"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"scripts": {
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover",
			"@php artisan elfinder:publish"
		]
	},
	"config": {
		"platform": {
			"php": "7.1.3"
		},
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
	}
}
