{
	"name": "ozdemirburak/laravel-5-simple-cms",
	"description": "Simple Laravel 5 content management system for starters.",
	"keywords": ["framework", "laravel", "cms"],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": "^7.0",
		"laravel/framework": "5.3.*",
		"laravelcollective/html": "5.3.*",
		"baum/baum": "~1.1",
		"kris/laravel-form-builder": "~1.7.0",
		"laracasts/flash": "~1.3",
		"cviebrock/eloquent-sluggable": "~4.0",
		"barryvdh/laravel-elfinder": "~0.3",
		"spatie/laravel-analytics": "~2.2",
		"caffeinated/menus": "~3.0",
		"yajra/laravel-datatables-oracle": "~6.0"
	},
	"require-dev": {
		"fzaninotto/faker": "~1.4",
		"mockery/mockery": "0.9.*",
		"phpunit/phpunit": "~5.0",
		"symfony/css-selector": "3.1.*",
		"symfony/dom-crawler": "3.1.*",
		"doctrine/dbal": "~2.5",
		"laracasts/generators": "~1.1",
		"barryvdh/laravel-ide-helper": "~2.0",
		"barryvdh/laravel-debugbar": "~2.0"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-root-package-install": [
			"php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		],
		"post-install-cmd": [
			"Illuminate\\Foundation\\ComposerScripts::postInstall",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"Illuminate\\Foundation\\ComposerScripts::postUpdate",
			"php artisan optimize",
			"php artisan ide-helper:generate",
			"php artisan elfinder:publish"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
